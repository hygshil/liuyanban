<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hp.dao.LiuDao">
	<!-- 添加 -->
	<insert id="insert">
		insert into liuyanban(title,author,content) values(#{title},#{author},#{content})
	</insert>
	<!-- 全查 -->
	<select id="selectAll" resultType="LiuYanBan">
		select * from liuyanban
	</select>
</mapper>